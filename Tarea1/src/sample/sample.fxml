            <TextArea fx:id="nom1" layoutX="168.0" layoutY="72.0" prefHeight="30.0" prefWidth="164.0" promptText="Introduzca su nombre" />
            <TextArea fx:id="edad" layoutX="168.0" layoutY="117.0" prefHeight="30.0" prefWidth="164.0" promptText="Introduzca su edad" />
            <Button fx:id="boton" layoutX="223.0" layoutY="184.0" mnemonicParsing="false" onAction="#pulsar" text="Votar" />
            <Label fx:id="confir" layoutX="168.0" layoutY="252.0" prefWidth="127.0" />
         </children>
      </Pane>
   </children>
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
</GridPane>
